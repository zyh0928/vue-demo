{
  "[svg]": {
    "editor.defaultFormatter": "jock.svg"
  },
  "[vue]": {
    "editor.defaultFormatter": "Vue.volar"
  },
  "diffEditor.hideUnchangedRegions.enabled": true,
  "diffEditor.ignoreTrimWhitespace": true,
  "eslint.validate": [
    "github-actions-workflow",
    "javascript",
    "javascriptreact",
    "json",
    "typescript",
    "typescriptreact",
    "vue",
    "yaml"
  ],
  "i18n-ally.extract.keygenStyle": "PascalCase",
  "i18n-ally.keystyle": "nested",
  "i18n-ally.localesPaths": "locales",
  "i18n-ally.sourceLanguage": "zh",
  "i18n-ally.theme.annotation": "#6a4c9c",
  "i18n-ally.theme.annotationBorder": "rgba(106, 76, 156, 0.5)",
  "i18n-ally.theme.annotationMissing": "#cb1b45",
  "i18n-ally.theme.annotationMissingBorder": "rgba(203, 27, 69, 0.5)",
  "yaml.schemas": {
    "@types/schema/i18n.json": "locales/*.yaml"
  }
}
